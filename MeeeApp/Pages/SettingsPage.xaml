<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MeeeApp.Pages.SettingsPage"
             Title="Settings">

    <VerticalStackLayout Margin="12,0,12,0">
        <Label Text="Settings" />
        <Button x:Name="BtnWipeCheckInTimes"
                Margin="0,12,0,12"
                Text="Wipe Check In Times"
                Clicked="BtnWipeCheckInTimes_OnClicked"></Button>
        <Button x:Name="BtnLogout"
                Text="Logout"
                Clicked="BtnLogout_OnClicked"></Button>
    </VerticalStackLayout>
    

</ContentPage>
